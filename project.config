[project]
	description = Doom 3 GPL source release
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/William Gatens (sadger)
[access "refs/for/refs/*"]
	read = group user/William Gatens (sadger)
	push = group user/William Gatens (sadger)
	label-Code-Review = -2..+2 group user/William Gatens (sadger)
	label-Verified = -1..+1 group user/William Gatens (sadger)
	rebase = group user/William Gatens (sadger)
	removeReviewer = group user/William Gatens (sadger)
	submit = group user/William Gatens (sadger)
[access "refs/heads/*"]
	read = group user/William Gatens (sadger)
	create = group user/William Gatens (sadger)
	push = +force group user/William Gatens (sadger)
	pushMerge = group user/William Gatens (sadger)
[access "refs/tags/*"]
	push = +force group user/William Gatens (sadger)
	pushTag = group user/William Gatens (sadger)
	pushSignedTag = group user/William Gatens (sadger)
